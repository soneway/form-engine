{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 3 Chrome versions",
            "last 3 Firefox versions",
            "Safari >= 10",
            "Explorer >= 9",
            "Edge >= 12"
          ]
        },
        "useBuiltIns": true,
        "modules": false
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "transform-vue-jsx"
  ]
}
